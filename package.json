{
  "name": "musiwork",
  "version": "1.0.0",
  "description": "A virtual music studio for students to learn and refine their craft.",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --public-url /js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "cleanup": "rm -rf .parcel-cache dist",
    "devc": "npm run cleanup && parcel ./index.html"
